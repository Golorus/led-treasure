    "main": "export var isBuzzerHit = false\nexport var treasurePlace = 0\nexport var buzzerPosition = 10\n\nexport var leader = 0\ndirection = 1\npixels = array(pixelCount)\ntreasurePlace = 20\nspeed = 0.2\n\n\nexport function beforeRender(delta) {\n  leader = (leader + direction * speed)\n  if (leader >= pixelCount) {\n    direction = -direction\n  }\n  if (leader <=0) {\n    direction = -direction\n  }\n  if (isBuzzerHit) {\n  buzzerPosition = floor(leader)\n  isBuzzerHit = false\n}\n  for (i=0; i<pixelCount;i++) {\n    if (i == floor(leader)) {\n      pixels[i] = 1\n      if (i>1) {\n        pixels[i-1] = 0.5\n        pixels[i-2] = 0.2\n      }\n      if (i<(pixelCount-2)) {\n        pixels[i+1] = 0.5\n        pixels[i+2] = 0.1\n      }\n    }else if (i == treasurePlace) {\n      pixels[i] =0.5\n    }else if(i== buzzerPosition) {\n      pixels[i] = 1\n    }\n    else{\n    pixels[i] = 0\n    }\n      \n    }\n}\n\n\nexport function render(index) {\n  v = pixels[index]\n  if (index == treasurePlace) {\n    h = 0.3\n  }else if(index == buzzerPosition) {\n    h = 0.6\n  }\n  else{\n    h= 0.4\n  }\n\n  hsv(h,0.9,v)\n}"
